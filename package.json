{
  "name": "sensible-maybe",
  "version": "2.2.0",
  "description": "A really small maybe function",
  "repository": "https://github.com/rzane/maybe",
  "author": "Ray Zane",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "pack build",
    "lint": "tsc -p . --noEmit",
    "format": "prettier --write '{src,tests}/**/*.ts'",
    "docs": "typedoc --options typedoc.config.json"
  },
  "jest": {
    "preset": "ts-jest"
  },
